<h3>Visits</h3>
<%# raise %>
<%= Browser.all.inspect %>
<table>
  <thead>
  </thead>
    <th>Total visits</th>
    <th>Unique vistors</th>
    <th>Page views</th>
    <!-- <th>Average visit time</th> -->
  <tbody>
    <tr>
      <td id="total_visits">
        <%= Visitor.total_visits %>
      </td>
      <td id="unique_visits">
        <%= @total_visits %>
      </td>
      <td>
        <%= Visitor.total_pageviews %>
      </td>
    </tr>
  </tbody>
</table>

<canvas id="pie_chart", height= '300', width='300'></canvas>
<div id="analytics">
  <h3>Browsers used</h3>
  <table>
    <thead>
      <th>Chrome</th>
      <th>Firefox</th>
      <th>Safari</th>
      <th>Internet Explorer</th>
      <th>Other</th>
    </thead>
    <tbody>
      <tr>
        <td><%=@chrome%></td>
        <td><%=@firefox%></td>
        <td><%=@safari%></td>
        <td><%=@ie%></td>
        <td><%=@other%></td>

      </tr>
    </tbody>
  </table>

</div>
<hr>

